2.Fibonacci Series:
int main()
{
  int n;
  scanf("%d",&n);
  int array[n];
  int a=0,b=1;
  if(n==0||n==1){
      printf("%d",1);
  }
  else{
      printf("%d %d ",a,b);
      for(int i=2;i<n;i++){
          int temp=a+b;
          a=b;
          b=temp;
          printf("%d ",temp);
      }
  }
__________________________________________________________________________________
3.Nth digit in Fibonacci series:
int main()
{
  int n;
  scanf("%d",&n);
  int array[n];
  int a=0,b=1,total=0;
  array[0]=a;
  array[1]=b;
  for(int i=2;i<n;i++){
      array[i]=a+b;
      a=b;
      b=array[i];
  }
  printf("%d",array[n-1]);
}
__________________________________________________________________________________
4.Sum of Fibonacci series:
int main()
{
  int n;
  scanf("%d",&n);
  int array[n];
  int a=0,b=1,total=0;
  array[0]=a;
  array[1]=b;
  for(int i=2;i<n;i++){
      array[i]=a+b;
      a=b;
      b=array[i];
  }
  for(int i=0;i<n;i++){
      total+=array[i];
  }
  printf("%d",total);
}
__________________________________________________________________________________
5.Armstrong:
#include <stdio.h>
#include <math.h>
int main()
{
  int n;
  scanf("%d",&n);
  int cmp=n;
  int temp=n;
  int count=0;
  int total=0;
  while(temp>0){
      temp/=10;
      count++;
  }
  while(cmp>0){
      int rev=cmp%10;
      total+=pow(rev,count);
      cmp/=10;
  }
  if(n==total){
      printf("True");
  }
 }
__________________________________________________________________________________
6.Palindrome:
int main()
{
  int n;
  scanf("%d",&n);
  int cmp=n;
  int rev=0;
  while(n>0){
      int temp=n%10;
      rev=rev*10+temp;
      n=n/10;
  }
  if(rev==cmp){
        printf("Palindrome");
  }
  else{
      printf("Not a Palindrome");
  }
 }
__________________________________________________________________________________
7.Prime:
int main()
{
  int n;
  scanf("%d",&n);
  int flag=0;
  for(int i=1;i<=n;i++){
     if(n%i==0){
         flag++;
     } 
  }
  if(flag==2){
      printf("It is a Prime");
  }
  else{
     printf("It is not a Prime");

  }
 }
