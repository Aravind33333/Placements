9.12345
   2345
    345
     45
      5


int main()
{
  int n;
  scanf("%d",&n);
  for(int i=1;i<=n;i++){
      for(int j=1;j<i;j++){
          printf(" ");
      }
      for(int k=i;k<=n;k++){
          printf("%d",k);
      }
      printf("\n");
  }
 }
